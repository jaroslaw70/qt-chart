INCLUDEPATH += $$[QT_INSTALL_HEADERS]/QtCommercialChart

CONFIG(debug, debug|release) {

    mac: {
        LIBS += -lQtCommercialChart_debug
    }
    
    win32: {
        LIBS += -lQtCommercialChartd
    }
    
    linux: {
        LIBS += -lQtCommercialChart
    }
    
} else {
    LIBS += -lQtCommercialChart
}
